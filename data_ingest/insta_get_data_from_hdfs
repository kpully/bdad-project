#the commands in this script get the newest version of the specified files from hdfs in order to transport processed data back to my local computer for dashboarding

#queries
hadoop fs -get insta_cleaned/queries_detailed
hadoop fs -get insta_cleaned/queries

#top_commenters
hadoop fs -get insta_cleaned/top_commenters
hadoop fs -get insta_cleaned/top_commenters_comments

#cross_comments
hadoop fs -get insta_cleaned/user_brands_grouped_multi
hadoop fs -get insta_cleaned/comment_overlap

#comment_types
hadoop fs -get insta_cleaned/comment_types
